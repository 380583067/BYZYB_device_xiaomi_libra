<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- List of regexpressions describing the interface (if any) that represent tetherable Wifi interfaces.  If the device doesn't want to support tethering over Wifi this should be empty.  An example would be "softap.*" -->
    <string-array translatable="false" name="config_tether_wifi_regexs">
        <item>"wlan0"</item>
    </string-array>
    <!-- List of regexpressions describing the interface (if any) that represent tetherable Wifi P2P interfaces.  If the device doesn't want to support tethering over Wifi P2p this should be empty.  An example would be "p2p-p2p.*" -->
    <string-array translatable="false" name="config_tether_wifi_p2p_regexs">
        <item>"p2p0"</item>
    </string-array>
</resources>
